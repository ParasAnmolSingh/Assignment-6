print ("enter a word/sentence" )
phr= input ()

new_phr=""
for i in phr:
  if i!=" ":
    new_phr+=i. upper()

count=1
n=len(new_phr)
for j in range (n//2+1): 
  if new_phr[j]!=new_phr[n-1-j]:
     count=0
     break 
if count==1:
  print ("It's a palindrome" )
else:
   print ("It's not a palindrome")
